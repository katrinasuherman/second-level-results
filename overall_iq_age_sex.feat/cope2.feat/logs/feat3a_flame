</pre><hr>Higher-level stats<br><pre>

cat ../design.lcon | awk '{ print  }' > design.lcon

/Users/katy/fsl/bin/fslsplit mask tmpmask -z

/Users/katy/fsl/bin/fslsplit filtered_func_data tmpcope -z

/Users/katy/fsl/bin/fslsplit var_filtered_func_data tmpvarcope -z

/Users/katy/fsl/bin/fslsplit tdof_filtered_func_data tmptdof -z
/Users/katy/fsl/bin/flameo --cope=tmpcope0000 --vc=tmpvarcope0000 --dvc=tmptdof0000 --mask=tmpmask0000 --ld=stats0000 --dm=design.mat --cs=design.grp --tc=design.con  --runmode=fe
